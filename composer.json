{
    "name": "teamcoltra/php-video-to-gif",
    "type": "library",
    "description": "A video to GIF converter package creating a new GIF for every new subtitle line using PHP-FFMpeg and Imagick",
    "keywords": [
        "video",
        "gif",
        "subtitles",
        "converter",
        "php-ffmpeg",
        "imagick"
    ],
    "homepage": "https://github.com/teamcoltra/php-video-to-gif",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Peacock",
            "email": "teamcoltra@gmail.com"
        }
    ],
    "require": {
        "php-ffmpeg/php-ffmpeg": "^0.17.0",
        "ext-imagick": "*"
    },
    "autoload": {
        "psr-4": {
            "TeamColtra\\PhpVideoToGif\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "platform": {
            "ext-imagick": "*"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}